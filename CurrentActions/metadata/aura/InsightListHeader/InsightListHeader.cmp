<aura:component implements="flexipage:availableForAllPageTypes" controller="InsightsController">

<ltng:require styles="/resource/SLDS0121/assets/styles/salesforce-lightning-design-system-ltng.css,/resource/jquery_ui_css" scripts="/resource/awssdk" afterScriptsLoaded="{!c.doInit}" />


<aura:handler event="c:AssocEvent" action="{!c.assocSearch}"/>
<aura:attribute name="title" type="String" default="All"/>


<div class="slds-page-header" role="banner">
	<div class="slds-grid">
		<div class="slds-col slds-has-flexi-truncate">
			<p class="slds-text-heading--label">{!v.title}</p>
		</div>
	</div>
</div>

</aura:component>